<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="app.registro.reservaciones.domain">
	<class name="Factura" table="Factura">
  		<id name="id" column="id">
			<generator class="increment" />
		</id>
      	
      	<property name="numero" column="numero" />	
      	<property name="fecha" column="fecha" />
      	<property name="montoEuro" column="montoEuro" />
      	<property name="montoUsd" column="montoUsd" />
      	<property name="montoCuc" column="montoCuc" />
	</class>
</hibernate-mapping>